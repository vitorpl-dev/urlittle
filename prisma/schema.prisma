generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        String   @id @default(uuid())
  name      String
  email     String   @unique
  password  String
  urls      Url[]
  createdAt DateTime @default(now())

  @@map("users")
}

model Url {
  id        String   @id @default(uuid())
  origin    String
  uri       String   @unique @db.VarChar(4)
  createdAt DateTime @default(now())
  users     User?    @relation(fields: [userId], references: [id])
  userId    String?

  @@map("urls")
}
